{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"tsx server/index.ts\" \"vite\"",
    "dev:client": "vite",
    "dev:server": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:client": "vite build",
    "start": "NODE_ENV=production node dist/index.js",
    "vercel-build": "npm run build:client",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx server/db/migrations/run-migration.ts",
    "db:seed": "tsx server/db/seed.ts",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "verify": "node scripts/verify-environment.mjs",
    "db:migrate:force": "tsx server/db/migrations/run-migration.ts --force",
    "db:analyze": "tsx server/db/migrations/run-migration.ts --analyze-only",
    "db:validate": "tsx server/db/migrations/validate-migration.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:jest": "jest",
    "test:ocr": "vitest tests/ocr-*.spec.ts",
    "test:ocr-integration": "vitest tests/ocr-integration.spec.ts",
    "test:ocr-providers": "vitest tests/ocr-providers.spec.ts",
    "test:ocr-api": "vitest tests/ocr-api-endpoints.spec.ts",
    "test:ocr-validation": "vitest tests/ocr-validation.spec.ts",
    "test:ocr-full": "node scripts/run-ocr-tests.mjs",
    "test:ocr-quick": "node scripts/validate-ocr-quick.mjs",
    "test:ocr-report": "node scripts/run-ocr-tests.mjs --report",
    "e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx",
    "railway:deploy": "node scripts/railway-integration.js deploy",
    "railway:status": "node scripts/railway-integration.js status",
    "railway:monitor": "node scripts/railway-integration.js monitor",
    "railway:scale": "node scripts/railway-integration.js scale",
    "railway:logs": "node scripts/railway-integration.js logs",
    "railway:health": "node scripts/railway-integration.js health",
    "railway:setup": "node scripts/railway-integration.js setup",
    "railway:cleanup": "node scripts/railway-integration.js cleanup",
    "deploy": "bash scripts/deploy-to-railway.sh",
    "health-check": "node scripts/health-checks.js",
    "start-monitoring": "node scripts/railway-monitoring.js start",
    "start-scaling": "node scripts/railway-scaling.js start"
  },
